<a:application xmlns:a="https://github.com/pylonide/pylon">
    <a:tab skin="docktab" visible="false">
        <a:page id="pgMungit" caption="Mungit" style="background-color:#303030">
            <div id="mungit_div"></div>
            <a:bar border="0 0 0 0" visible="[{require('ext/settings/settings').model}::mungit/@top_bar]">
                <a:hbox class="mungitbar" height="36" width="100%" align="center" edge="4 5 4" padding="5">
                    <a:textbox id="txtMungit" style="width:100%" class="ace_searchbox tb_textbox searchbox searchTxt tb_console"
                        flex="1" value=""
                        onkeydown="if (event.keyCode == 13) require('ext/mungit/mungit').refresh()" />
                    <a:button onclick="require('ext/mungit/mungit').refresh()" skin="btn-mungit-nav" skinset="mungitskin" class="refresh"></a:button>
                    <a:divider skin="c9-divider" />
                    <a:button onclick="require('ext/mungit/mungit').popup()" skin="btn-mungit-nav" skinset="mungitskin" class="popup"></a:button>
                    <a:divider skin="c9-divider" />
                    <a:button onclick="require('ext/mungit/mungit').close()" skin="btn-mungit-nav" skinset="mungitskin" class="close"></a:button>
                </a:hbox>
            </a:bar>
            <iframe
                id="mungit_iframe"
                onload="require('ext/mungit/mungit').onLoad()"
                src="" />
        </a:page>
    </a:tab>
</a:application>
