<?xml version='1.0'?>
<a:skin xmlns:a="https://github.com/pylonide/pylon">
<a:button name="btn-mungit-nav">
    <a:style><![CDATA[
        .btn-mungit-nav {
          height             : 24px;
          overflow           : hidden;
          cursor             : default;
          position           : relative;
          background-repeat:no-repeat;
          background-position: 0 0;
          -moz-user-select: -moz-none;
          -khtml-user-select : none;
          -webkit-user-select: none;
          -o-user-select: none;
          user-select        : none;
          
          background: url(images/toolbar.png) repeat-x 0 100px;
          border-radius: 3px;

          border: 1px solid transparent;
        }

        .btn-mungit-navOver {
          border-color: #010910;
        }

        .btn-mungit-navDown {
          background-position : 0 -36px;
          border-color: #010910;
        }

        .btn-mungit-nav .icon {
          width                 : 24px;
          height                : 23px;
          -webkit-border-radius : 3px;
          -moz-border-radius    : 3px;
          border-radius         : 3px;
          margin-left: 1px;
        }
        
        .btn-mungit-nav.close .icon {
            position: relative;
        }
        
        .btn-mungit-nav.close .icon:before {
          content: "\f00d";
          font-family: FontAwesome;
          color: #fff;
          left:6px;
          position:absolute;
          top:6px;
        }
        
        .btn-mungit-nav.refresh .icon {
            position: relative;
        }
        
        .btn-mungit-nav.refresh .icon:before {
          content: "\f021";
          font-family: FontAwesome;
          color: #fff;
          left:6px;
          position:absolute;
          top:6px;
        }
        
        .btn-mungit-nav.popup .icon {
            position: relative;
        }
        
        .btn-mungit-nav.popup .icon:before {
          content: "\f077";
          font-family: FontAwesome;
          color: #fff;
          left:6px;
          position:absolute;
          top:6px;
        }
    ]]></a:style>

     <a:presentation>
        <a:main caption="div" label="div" background="." icon="div">
            <div class="btn-mungit-nav">
                <div class="icon"/>
            </div>
        </a:main>
    </a:presentation>
</a:button>
</a:skin>
